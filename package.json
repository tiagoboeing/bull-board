{
  "name": "@tiagoboeing/bull-board",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "description": "Bull queue UI for inspecting jobs",
  "keywords": [
    "bull",
    "bullmq",
    "redis",
    "queue",
    "monitoring",
    "dashboard",
    "docker"
  ],
  "bugs": {
    "url": "https://github.com/tiagoboeing/bull-board/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagoboeing/bull-board.git"
  },
  "license": "MIT",
  "author": "Tiago Boeing <contato@tiagoboeing.com>",
  "scripts": {
    "lint": "eslint \"./**/*.ts*\"",
    "build": "tsc",
    "start:dev": "ts-node-dev --rs src/index.ts",
    "start:docker": "docker-compose up -d",
    "release": "release-it --only-version",
    "test": "jest"
  },
  "dependencies": {
    "@bull-board/api": "^4.8.0",
    "@bull-board/express": "^4.8.0",
    "ansis": "^1.5.5",
    "bull": "^4.9.0",
    "bullmq": "^1.90.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.17.9",
    "@release-it/bumper": "^4.0.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.15",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/redis-info": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "release-it": "^15.4.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "release-it": {
    "plugins": {
      "@release-it/bumper": {
        "in": "package.json",
        "out": "package.json"
      }
    },
    "git": {
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}",
      "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "hooks": {
      "before:init": [
        "npm run lint"
      ],
      "after:bump": [
        "npx auto-changelog -p",
        "npm run build"
      ]
    },
    "github": {
      "release": true
    },
    "npm": false
  }
}
